{
  "id": "{AKS_CLUSTER_ID}",
  "name": "{CLUSTER_NAME}",
  "location": "{AZURE_LOCATION}",
  "type": "Microsoft.ContainerService/ManagedClusters",
  "properties": {
    "kubernetesVersion": "{KUBERNETES_VERSION}",
    "dnsPrefix": "aks",
    "agentPoolProfiles": [
      {
        "name": "systempool",
        "count": 3,
        "mode" : "System",
        "vmSize": "Standard_DS2_v2",
        "osType": "Linux",
        "availabilityProfile": "VirtualMachineScaleSets",
        "enableAutoScaling": true,
        "maxCount": 100,
        "minCount": 3
      },
      {
        "name": "userpool1",
        "count": 3,
        "mode" : "User",
        "vmSize": "Standard_DS2_v2",
        "osType": "Linux",
        "availabilityProfile": "VirtualMachineScaleSets",
        "enableAutoScaling": true,
        "maxCount": 100,
        "minCount": 3
      },
      {
        "name": "userpool2",
        "count": 3,
        "mode" : "User",
        "vmSize": "Standard_DS2_v2",
        "osType": "Linux",
        "availabilityProfile": "VirtualMachineScaleSets",
        "enableAutoScaling": true,
        "maxCount": 100,
        "minCount": 3
      }
    ],
    "encodedCustomConfiguration": "{CUSTOM_CONFIG}"
  }
}
